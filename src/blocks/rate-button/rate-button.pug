mixin rate-button({ rating = 0, maxRating = 5 })
  - const iconOutlined = maxRating - rating;

  ul.rate-button
    - for (let x = 0; x < rating; x++)
      li.rate-button__item
        i.rate-button__icon star

    - for (let x = 0; x < iconOutlined; x++)
      li.rate-button__item
        i.rate-button__icon star_border
